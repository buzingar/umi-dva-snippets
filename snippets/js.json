    "body": [
      "export const makeId = (len) => {",
      "\tlet text = '';",
      "\tlet possible = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';",
      "\tfor(let i = 0; i < len; i += 1) {",

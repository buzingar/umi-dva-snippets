  "shuffle": {
    "prefix": "gn-util-shuffle",
    "body": [
      "export const shuffle = ([…array]) => {",
      "\t let m = array.length;",
      "",
      "\t while(m){",
      "\t\t const i = Math.floor(Math.random() * m);",
      "\t\t [array[m], array[i]] = [array[i], array[m]]",
      "\t }",
      "\t return array",
      "};"
    ],
    "description": "随机播放播放项目"
  },
